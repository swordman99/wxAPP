server {
    listen  80;
    server_name localhost; 

    location / { 
include      uwsgi_params;
uwsgi_pass   127.0.0.1:5000;
uwsgi_param UWSGI_PYHOME /home/ubuntu/flasky-first-edtion; # 指向虚拟环境目录
uwsgi_param UWSGI_CHDIR  /home/ubuntu/flasky-first-edtion; # 指向网站根目录
uwsgi_param UWSGI_SCRIPT manage:app; # 指定启动程序
    }
}